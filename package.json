{
  "name": "brandonwkipp.com",
  "private": true,
  "author": "Brandon W. Kipp <brandonwkipp@gmail.com>",
  "scripts": {
    "build": "gatsby clean && gatsby build",
    "checkyarn": "echo \"$npm_execpath\" | grep -q \"yarn\\.js$\" || (echo '⚠️ Please use yarn instead of npm! ⚠️ ' && echo && exit 1)",
    "clean": "gatsby clean",
    "cy:gui-deploy": "cypress open --env environment=deployment",
    "cy:gui-develop": "cypress open --env environment=development",
    "cy:run-deploy": "cypress run --env environment=deployment",
    "cy:run-develop": "cypress run --env environment=development",
    "depcheck": "node -r @babel/register scripts/depcheck.js",
    "develop": "gatsby develop --port 4000",
    "fetch:cv": "node scripts/fetch-cv.js",
    "preinstall": "$npm_execpath --silent run checkyarn",
    "postinstall": "npm-run-all setup && node_modules/.bin/sort-package-json && yarn depcheck",
    "lint": "npm-run-all lint:*",
    "lint:code": "node_modules/.bin/eslint .",
    "lint:style": "node_modules/.bin/stylelint \"**/*.{*css,js}\"",
    "serve": "gatsby serve --port 4000",
    "setup": "npm-run-all setup:*",
    "setup:development": "cp -n env.example .env.development || true",
    "setup:production": "cp -n env.example .env.production || true"
  },
  "husky": {
    "hooks": {
      "pre-push": "yarn lint"
    }
  },
  "dependencies": {
    "@contentful/rich-text-types": "^14.1.1",
    "@fortawesome/fontawesome-svg-core": "^1.2.30",
    "@fortawesome/free-brands-svg-icons": "^5.14.0",
    "@fortawesome/free-regular-svg-icons": "^5.14.0",
    "@fortawesome/free-solid-svg-icons": "^5.14.0",
    "@fortawesome/react-fontawesome": "^0.1.11",
    "axios": "^0.21.1",
    "bootstrap": "^4.5.2",
    "countdown": "^2.6.0",
    "dotenv": "^8.2.0",
    "gatsby": "^3.0.4",
    "gatsby-plugin-exclude": "^1.0.2",
    "gatsby-plugin-image": "^1.0.0",
    "gatsby-plugin-manifest": "^3.0.0",
    "gatsby-plugin-offline": "^4.0.0",
    "gatsby-plugin-react-helmet": "^4.0.0",
    "gatsby-plugin-sass": "^4.0.0",
    "gatsby-plugin-sharp": "^3.0.1",
    "gatsby-plugin-typography": "^3.0.0",
    "gatsby-source-contentful": "^5.0.3",
    "gatsby-transformer-sharp": "^3.0.0",
    "html-loader": "^1.3.2",
    "markdown-loader": "^6.0.0",
    "marked": "^2.0.0",
    "moment": "^2.29.1",
    "moment-countdown": "^0.0.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-burger-menu": "^2.9.0",
    "react-dom": "^16.10.2",
    "react-helmet": "^5.2.1",
    "react-typography": "^0.16.19",
    "reactstrap": "^8.7.1",
    "typography": "^0.16.19",
    "typography-theme-st-annes": "^0.16.19",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/register": "^7.12.13",
    "chalk": "^4.1.0",
    "cypress": "^5.6.0",
    "cypress-axe": "^0.10.0",
    "depcheck": "^1.3.1",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-cypress": "^2.10.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "gatsby-cypress-endpoints": "^0.0.6",
    "gatsby-plugin-eslint": "^2.0.8",
    "husky": "^5.1.3",
    "lodash": "^4.17.20",
    "node-sass": "^4.14.1",
    "npm-run-all": "^4.1.5",
    "sort-package-json": "^1.48.1",
    "stylelint": "^13.7.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-order": "^4.1.0",
    "yamljs": "^0.3.0"
  },
  "engines": {
    "node": ">=14.15.5"
  },
  "engineStrict": true
}
