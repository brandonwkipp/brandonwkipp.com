{
  "name": "hidalgo-rides/intersect-framework",
  "description": "Web framework built for making dev life a little easier.",
  "type": "project",
  "license": "MIT",
  "keywords": [
    "intersect",
    "framework",
    "hidalgo"
  ],
  "autoload" : {
    "psr-4" : {
      "App\\" : "src/"
    }
  },
  "autoload-dev" : {
    "psr-4" : {
      "Tests\\" : "tests/"
    }
  },
  "require": {
    "php": "^7.1.3",
    "hidalgo-rides/intersect-blog": "^1.0",
    "hidalgo-rides/intersect-web": "^1.0",
    "twig/twig": "^2.0",
    "webonyx/graphql-php": "^0.13.8"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "config": {
    "discard-changes": true,
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  }
}
